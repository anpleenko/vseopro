div(block="search-result-meta")
  ul(elem="list").lsn.pl0.mb0
    li(elem="item").dib
      span(elem="icon").icon-user
      a(href="#" elem="link") #{getUserName()}
    li(elem="item").dib
      span(elem="icon").icon-calendar
      a(href="#" elem="link") #{getRandomDate()}
    li(elem="item").dib
      span(elem="icon").icon-eye
      a(href="#" elem="link") #{getRandomInt(1300, 15)}
    li(elem="item").dib
      span(elem="icon").icon-comment
      a(href="#" elem="link") #{getRandomInt(1300, 15)}
    li(elem="item").dib
      span(elem="icon").icon-folder-open
      a(href="#" elem="link" mods="breadcrumbs") Шаблоны

